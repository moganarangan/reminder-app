{
  "expo": {
    "name": "My Reminder",
    "slug": "reminder-app",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#27B4E8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "androidNavigationBar": {
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#27B4E8"
    },
    "ios": {
      "bundleIdentifier": "com.own.myreminder",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.own.myreminder",
      "versionCode": 1
    },
    "notification": {
      "icon": "./assets/icon3.png",
      "iosDisplayInForeground": true,
      "color": "#27B4E8",
      "androidMode": "default"
    }
  }
}